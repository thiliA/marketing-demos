<div class="content" id="post" name="post">
<h3>post()</h3>

<pre class="prettyprint linenums">
&lt%
var url = 'http://jaggeryjs.org/apidocs/resources/data.jag';
var result = post(url, 'Jaggery HTTP POST', {
    "User-Agent" : "Jaggery-XHR",
    "Country" : "LK"
}, 'json');
print(result.data);
%&gt
</pre>
<%
var emailCode = '&lt%'
+ "\n var url = 'http://jaggeryjs.org/apidocs/resources/data.jag';"
+ "\n var result = post(url, 'Jaggery HTTP POST', {"
